{
    "name": "relevanz/retargeting-oxid-plugin",
    "description": "releva.nz Retargeting for oxid-6",
    "require": {
        "php": "~5.5.0|~5.6.0|~7.1.3|~7.2.0"
    },
    "type": "oxideshop-module",
    "extra": {
        "oxideshop": {
            "target-directory": "relevanz/retargeting",
            "blacklist-filter": [
                "composer.json",
                "README.md",
                "Controller/*",
                "Controller/*/*",
                "Model/*"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Relevanz\\RetargetingOxid\\": "./"
        }
    },
    "version": "1.0.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ]
}
